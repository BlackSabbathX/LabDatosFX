<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<?import java.net.URL?>
<AnchorPane id="AnchorPane" prefHeight="448.0" prefWidth="573.0" styleClass="mainFxmlClass"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Ventana.PrincipalUsuario.Agregar.Vacante.AgregarV">
    <stylesheets>
        <URL value="@AgregarV.css"/>
    </stylesheets>
    <children>
        <StackPane fx:id="content" prefHeight="458.0" prefWidth="586.0" style="-fx-background-color: #ffffff;">
            <children>
                <Pane prefHeight="221.0" prefWidth="302.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="48.0" prefWidth="586.0" text="Agregar Vacante"
                               textFill="#2c3e50">
                            <font>
                                <Font name="Segoe UI Bold" size="19.0"/>
                            </font>
                        </Label>
                        <JFXTextField fx:id="nombre" alignment="CENTER" focusColor="#2c3e50" layoutX="15.0"
                                      layoutY="70.0" prefHeight="28.0" prefWidth="261.0" promptText="Nombre"
                                      style="-fx-text-fill: #2c3e50;" unFocusColor="#2c3e50">
                            <font>
                                <Font name="Segoe UI" size="14.0"/>
                            </font>
                        </JFXTextField>
                        <Pane layoutY="419.0" prefHeight="39.0" prefWidth="586.0"
                              style="-fx-background-color: #2c3e50;">
                            <children>
                                <JFXButton layoutX="14.0" layoutY="7.0" onAction="#cancelar"
                                           style="-fx-background-color: #ffffff;" text="Cancelar" textFill="#2c3e50">
                                    <font>
                                        <Font name="Segoe UI" size="13.0"/>
                                    </font>
                                </JFXButton>
                                <JFXButton layoutX="505.0" layoutY="6.0" onAction="#agregar" prefHeight="27.0"
                                           prefWidth="67.0" style="-fx-background-color: #ffffff;" text="Agregar"
                                           textFill="#2c3e50">
                                    <font>
                                        <Font name="Segoe UI" size="13.0"/>
                                    </font>
                                </JFXButton>
                            </children>
                        </Pane>
                        <JFXTextArea fx:id="descripcion" focusColor="#2c3e50" layoutX="310.0" layoutY="70.0"
                                     prefHeight="108.0" prefWidth="261.0" promptText="Descripción"
                                     style="-fx-text-fill: #2c3e50; -fx-text-alignment: CENTER;" unFocusColor="#2c3e50">
                            <font>
                                <Font name="Segoe UI" size="14.0"/>
                            </font>
                        </JFXTextArea>
                        <JFXComboBox fx:id="empresa" focusColor="#2c3e50" layoutX="15.0" layoutY="117.0"
                                     prefHeight="25.0" prefWidth="261.0" promptText="Empresa"
                                     style="-fx-prompt-text-alignment: CENTER;" unFocusColor="#2c3e50"/>
                        <JFXComboBox fx:id="jornada" focusColor="#2c3e50" layoutX="15.0" layoutY="157.0"
                                     prefHeight="25.0" prefWidth="261.0" promptText="Jornada"
                                     style="-fx-prompt-text-alignment: CENTER;" unFocusColor="#2c3e50"/>
                        <JFXTextField fx:id="maximo" alignment="CENTER" focusColor="#2c3e50" layoutX="310.0"
                                      layoutY="215.0" prefHeight="28.0" prefWidth="261.0" promptText="Salario máximo"
                                      style="-fx-text-fill: #2c3e50;" unFocusColor="#2c3e50">
                            <font>
                                <Font name="Segoe UI" size="14.0"/>
                            </font>
                        </JFXTextField>
                        <JFXTextField fx:id="minimo" alignment="CENTER" focusColor="#2c3e50" layoutX="16.0"
                                      layoutY="215.0" prefHeight="28.0" prefWidth="261.0" promptText="Salario minimo"
                                      style="-fx-text-fill: #2c3e50;" unFocusColor="#2c3e50">
                            <font>
                                <Font name="Segoe UI" size="14.0"/>
                            </font>
                        </JFXTextField>
                        <ScrollPane hbarPolicy="NEVER" layoutX="14.0" layoutY="301.0" prefHeight="108.0"
                                    prefWidth="261.0" style="-fx-background-color: #fff;">
                            <content>
                                <!--<Pane prefHeight="106.0" prefWidth="259.0" style="-fx-background-color: #fff;">-->
                                <!--<children>-->
                                <VBox fx:id="titulos" prefHeight="105.0" prefWidth="261.0"
                                      style="-fx-background-color: #fff;"/>
                                <!--</children>-->
                                <!--</Pane>-->
                            </content>
                        </ScrollPane>
                        <ScrollPane hbarPolicy="NEVER" layoutX="310.0" layoutY="301.0" prefHeight="108.0"
                                    prefWidth="261.0" style="-fx-background-color: #fff;">
                            <content>
                                <!--<Pane prefHeight="106.0" prefWidth="259.0" style="-fx-background-color: #fff;">-->
                                <!--<children>-->
                                <VBox fx:id="habilidades" prefHeight="104.0" prefWidth="261.0"
                                      style="-fx-background-color: #fff;"/>
                                <!--</children>-->
                                <!--</Pane>-->
                            </content>
                        </ScrollPane>
                        <JFXComboBox fx:id="titulo" focusColor="#2c3e50" layoutX="14.0" layoutY="264.0"
                                     prefHeight="25.0" prefWidth="261.0" promptText="Titulo"
                                     style="-fx-prompt-text-alignment: CENTER;" unFocusColor="#2c3e50"/>
                        <JFXComboBox fx:id="habilidad" focusColor="#2c3e50" layoutX="310.0" layoutY="264.0"
                                     prefHeight="27.0" prefWidth="261.0" promptText="Habilidad"
                                     style="-fx-prompt-text-alignment: CENTER;" unFocusColor="#2c3e50"/>
                    </children>
                </Pane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
