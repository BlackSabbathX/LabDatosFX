<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.effect.*?>
<?import javafx.geometry.*?>
<?import com.jfoenix.controls.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="231.0" prefWidth="302.0" styleClass="mainFxmlClass"
            xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Ventana.PrincipalUsuario.Agregar.Empresa.AgregarE">
    <stylesheets>
        <URL value="@AgregarE.css"/>
    </stylesheets>
    <children>
        <StackPane fx:id="content" prefHeight="238.0" prefWidth="302.0" style="-fx-background-color: #ffffff;">
            <children>
                <Pane prefHeight="221.0" prefWidth="302.0">
                    <children>
                        <Label alignment="CENTER" prefHeight="48.0" prefWidth="302.0" text="Agregar Empresa"
                               textFill="#2c3e50">
                            <font>
                                <Font name="Segoe UI Bold" size="19.0"/>
                            </font>
                        </Label>
                        <JFXTextField fx:id="nombre" alignment="CENTER" focusColor="#2c3e50" layoutX="21.0"
                                      layoutY="70.0" prefHeight="28.0" prefWidth="261.0" promptText="Nombre"
                                      style="-fx-text-fill: #2c3e50;" unFocusColor="#2c3e50">
                            <font>
                                <Font name="Segoe UI" size="14.0"/>
                            </font>
                        </JFXTextField>
                        <JFXTextField fx:id="telefono" alignment="CENTER" focusColor="#2c3e50" layoutX="21.0"
                                      layoutY="122.0" prefHeight="28.0" prefWidth="261.0" promptText="Telefono"
                                      style="-fx-text-fill: #2c3e50;" unFocusColor="#2c3e50">
                            <font>
                                <Font name="Segoe UI" size="14.0"/>
                            </font>
                        </JFXTextField>
                        <Pane layoutY="199.0" prefHeight="39.0" prefWidth="302.0"
                              style="-fx-background-color: #2c3e50;">
                            <children>
                                <JFXButton layoutX="14.0" layoutY="7.0" onAction="#cancelar"
                                           style="-fx-background-color: #ffffff;" text="Cancelar" textFill="#2c3e50">
                                    <font>
                                        <Font name="Segoe UI" size="13.0"/>
                                    </font>
                                </JFXButton>
                                <JFXButton layoutX="220.0" layoutY="7.0" onAction="#agregar" prefHeight="27.0"
                                           prefWidth="67.0" style="-fx-background-color: #ffffff;" text="Agregar"
                                           textFill="#2c3e50">
                                    <font>
                                        <Font name="Segoe UI" size="13.0"/>
                                    </font>
                                </JFXButton>
                            </children>
                        </Pane>
                    </children>
                </Pane>
            </children>
        </StackPane>
    </children>
</AnchorPane>
